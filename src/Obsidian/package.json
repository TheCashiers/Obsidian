{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^2.7.6",
    "@types/jest": "^19.2.2",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-test-renderer": "^15.4.2",
    "coveralls": "^2.12.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "happypack": "^3.0.3",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-jest": "^19.0.5",
    "ts-loader": "^0.8.2",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/Scripts/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:travis": "npm run test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "start": "webpack"
  },
  "dependencies": {
    "@types/jquery": "^2.0.40",
    "@types/react": "^15.0.11",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.4",
    "axios": "^0.13.1",
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.4.2",
    "react-router": "^2.8.1",
    "snackbarjs": "^1.1.0"
  }
}
